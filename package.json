{
  "name": "enhanced-pdf-generator",
  "version": "1.0.0",
  "description": "Production-ready HTML and Text to PDF converter with zero external dependencies",
  "main": "enhanced-pdf-generator.js",
  "types": "enhanced-pdf-generator.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node production-demo.js",
    "test": "node production-demo.js",
    "test:quick": "node -e \"require('./production-demo').runBasicTests()\"",
    "demo": "node production-demo.js",
    "validate": "node -e \"console.log('Validating Enhanced PDF Generator...'); const gen = require('./enhanced-pdf-generator'); console.log('âœ… Module loaded successfully')\"",
    "benchmark": "node -e \"require('./production-demo').runBenchmark()\"",
    "lint": "echo 'No linting configured - using production-ready code'",
    "clean": "rm -rf output && mkdir -p output"
  },
  "keywords": [
    "pdf",
    "html-to-pdf",
    "text-to-pdf",
    "pdf-generator",
    "zero-dependencies",
    "production-ready",
    "enterprise",
    "banking",
    "document-generation",
    "cross-platform",
    "nodejs",
    "typescript-ready"
  ],
  "author": {
    "name": "Tushar Kar",
    "email": "tushar.et1@gmail.com"
  },
  "contributors": [
    {
      "name": "Production Team",
      "email": "production@enhanced.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/enhanced-pdf-generator.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/enhanced-pdf-generator/issues"
  },
  "homepage": "https://github.com/your-org/enhanced-pdf-generator#readme",
  "dependencies": {},
  "devDependencies": {},
  "peerDependencies": {},
  "optionalDependencies": {},
  "files": [
    "enhanced-pdf-generator.js",
    "production-demo.js",
    "enhanced-pdf-generator.d.ts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "directories": {
    "test": "tests",
    "doc": "docs"
  },
  "config": {
    "defaultOutputDir": "./output",
    "maxMemoryUsage": "512MB",
    "supportedFormats": ["A4", "LETTER", "LEGAL", "A3", "TABLOID"]
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64",
    "ia32"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-org"
  },
  "security": {
    "policy": "https://github.com/your-org/enhanced-pdf-generator/security/policy"
  },
  "exports": {
    ".": {
      "require": "./enhanced-pdf-generator.js",
      "import": "./enhanced-pdf-generator.js",
      "types": "./enhanced-pdf-generator.d.ts"
    },
    "./package.json": "./package.json"
  },
  "browserslist": [
    "node >= 16.0.0"
  ],
  "volta": {
    "node": "18.19.0",
    "npm": "10.2.3"
  }
}